Xcursor.theme: DMZ-Black

URxvt.iconFile: /usr/share/pixmaps/urxvt.xpm
URxvt.iconName: xterm
URxvt.title: xterm

URxvt.perl-ext-common:  default,matcher,searchable-scrollback,clipboard
URxvt.cutchars:         "().,<>[]{}|'`
URxvt.termName:         rxvt-unicode
URxvt.urlLauncher:      x-www-browser
URxvt.matcher.button:   1
URxvt.saveLines:        4096
URxvt.visualBell: True
URxvt.urgentOnBell: true

URxvt.scrollTtyOutput: false
URxvt.scrollWithBuffer: true
URxvt.scrollTtyKeypress: true
!URxvt.secondaryScroll: false

!URxvt.keysym.S-Prior:   ^[[5;2~
!URxvt.keysym.S-Next:    ^[[6;2~

!! list of "perfect" 16:5 ratios: 80x25, 96x30, 112x35, 128x40
URxvt.geometry:      80x25
URxvt.highlightSelection: true
URxvt.transparent: false
URxvt.shading: 3
!URxvt*tintColor: #000000
!URxvt*blurRadius: 5

URxvt.scrollstyle:      plain
URxvt.secondaryScroll:  true

!! disp options
URxvt*cursorBlink:   off
URxvt*boldMode:      on
URxvt*underLine:     on

!! paste
URxvt.keysym.C-M-c: builtin-string:
URxvt.keysym.C-M-v: builtin-string:


! gohufont, small
! *font: -gohu-gohufont-medium-r-normal-*-11-80-100-100-c-60-iso10646-1 */
! *boldFont: -gohu-gohufont-medium-r-normal--11-80-100-100-c-60-iso10646-1
! *boldFont: -gohu-gohufont-bold-r-normal-*-11-80-100-100-c-60-iso10646-1

! gohufont, large
!*font: -gohu-gohufont-medium-r-normal-*-14-100-100-100-c-80-iso10646-1
!*boldFont: -gohu-gohufont-medium-r-normal-*-14-100-100-100-c-80-iso10646-1
!*boldFont: -gohu-gohufont-bold-r-normal-*-14-100-100-100-c-80-iso10646-1

! MonteCarlo
!*font: -*-montecarlo-medium-r-normal-*-11-110-72-72-c-60-*-*
!*boldFont: -*-montecarlo-medium-r-normal-*-11-110-72-72-c-60-*-*

! proggy
!*font: -*-proggysquare-medium-r-normal-*-11-80-96-96-c-70-iso8859-1
!*boldFont: -*-proggysquare-medium-r-normal-*-11-80-96-96-c-70-iso8859-1

! xft fonts
! Xft.antialias:	true 
! URxvt*font:     xft:Fantasque Sans Mono:pixelsize=12
! URxvt*font:     xft:DejaVu Sans Mono:pixelsize=10
! URxvt*boldFont:     xft:DejaVu Sans Mono:pixelsize=10

! small terminus fonts
!URxvt*font:     -xos4-terminus-medium-r-normal-*-12-120-72-72-c-60-iso10646-1
!URxvt*boldfont: -xos4-terminus-medium-r-normal-*-12-120-72-72-c-60-iso10646-1

! big terminus fonts
!URxvt*font:     -xos4-terminus-medium-r-normal-*-14-140-72-72-c-80-iso10646-1
!URxvt*boldfont: -xos4-terminus-medium-r-normal-*-14-140-72-72-c-80-iso10646-1

! huge terminus fonts
URxvt*font:     -xos4-terminus-medium-r-normal-*-18-*-*-*-*-*-iso10646-1
URxvt*boldFont: -xos4-terminus-bold-r-normal-*-18-*-*-*-*-*-iso10646-1

! colors -----------------------------------------------------------------
!! color options
URxvt*colorMode:     on
URxvt*colorBDMode:   on
URxvt*colorULMode:   on
!! term colors
URxvt*background:   #262522
URxvt*cursorColor:  #ff4f00
URxvt*foreground:   #e6994c
URxvt*pointerColor: #ff4f00
!! color palette ----------------------
!  black
URxvt*color0:		#1a1a1a
