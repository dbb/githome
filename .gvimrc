"set guifont=terminus\ 12
set guifont=Envy\ Code\ R\ 10
"set guifont=Monaco\ 10
"set guifont=Mensch\ Bold\ 9
"set guifont=Bowman\ 10
"set guifont=Inconsolata-dz\ 10
set lines=32 columns=84
set background=dark
set guioptions-=T
set showtabline=2

"colorscheme paintbox2
colorscheme codeburn

" horizontal lines
hi User1 guibg=#565656 guifg=#212121 gui=none
" char info
hi User2 guibg=#565656 guifg=#4c92d9 gui=none
" line, col
hi User3 guibg=#565656 guifg=#d9584c gui=none
" [+]
hi User4 guibg=#565656 guifg=#924cd9 gui=none
" file
hi User5 guibg=#565656 guifg=#67c439 gui=none
 
